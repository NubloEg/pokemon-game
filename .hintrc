{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-ms-text-size-adjust",
          "scale",
          "scrollbar-color",
          "scrollbar-width"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "no-inline-styles": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 117",
    "not chrome <= 117",
    "not edge <= 117",
    "not ios_saf <= 12.5",
    "not opera <= 102",
    "not samsung <= 22"
  ]
}